        {
          "entrypoint": "./lib/webparts/parentChild/ParentChildWebPart.js",
          "manifest": "./src/webparts/parentChild/ParentChildWebPart.manifest.json"
        }
      ]
    },
    "rest-api-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/restApi/RestApiWebPart.js",
